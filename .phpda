mode: 'usage'
source: '.'
filePattern: '*.php'
ignore: 'tests'
formatter: 'PhpDA\Writer\Strategy\Html'
target: './phpda.html'
groupLength: 1
visitor:
  - PhpDA\Parser\Visitor\TagCollector
  - PhpDA\Parser\Visitor\SuperglobalCollector
visitorOptions:
  PhpDA\Parser\Visitor\Required\DeclaredNamespaceCollector: {minDepth: 3, sliceLength: 3}
  PhpDA\Parser\Visitor\Required\MetaNamespaceCollector: {minDepth: 3, sliceLength: 3, excludePattern: '/^(?!.*CoreShop\/Component).*$/'}
  PhpDA\Parser\Visitor\Required\UsedNamespaceCollector: {minDepth: 3, sliceLength: 3, excludePattern: '/^(?!.*CoreShop\/Component).*$/'}
  PhpDA\Parser\Visitor\TagCollector: {minDepth: 3, sliceLength: 3, excludePattern: '/^(?!.*CoreShop\/Component).*$/'}