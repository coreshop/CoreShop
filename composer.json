  "require": {
    "php": "^8.0",
    "ext-json": "*",
    "doctrine/data-fixtures": "^1.5",
    "doctrine/doctrine-bundle": "^2.4",
    "doctrine/doctrine-fixtures-bundle": "^3.4",
    "doctrine/orm": "^2.9",
    "fakerphp/faker": "^1.16",
    "gedmo/doctrine-extensions": "^3.11",
    "jms/serializer-bundle": "^4.0 | ^5.0",
    "knplabs/knp-menu-bundle": "^3.1",
    "laminas/laminas-stdlib": "^3.6",
    "laminas/laminas-zendframework-bridge": "^1.5",
    "payum/payum": "^1.7",
    "payum/payum-bundle": "^2.6",
    "php-http/guzzle7-adapter": "^1.0",
    "php-http/message-factory": "^1.1",
    "pimcore/admin-ui-classic-bundle": "^1.2",
    "pimcore/google-marketing-bundle": "^1.0",
    "pimcore/newsletter-bundle": "^1.0",
    "pimcore/pimcore": "^11.1",
    "pimcore/web-to-print-bundle": "^1.3",
    "rinvex/countries": "^7.3",
    "sebastian/diff": "^4.0 | ^5.0 | ^6.0",
    "stof/doctrine-extensions-bundle": "^1.6",
    "sylius/theme-bundle": "^2.2",
    "symfony/dotenv": "^6.3",
    "symfony/http-client": "^6.3",
    "symfony/intl": "^6.3",
    "symfony/notifier": "^6.3",
    "symfony/serializer": "^6.3",
    "symfony/twig-bundle": "^6.3",
    "symfony/form": "^6.3",
    "webmozart/assert": "^1.10"
  },
  "require-dev": {
    "behat/behat": "^3.8",
    "behat/mink": "^1.8",
    "chrome-php/chrome": "^1.11",
    "dbrekelmans/bdi": "^1.0",
    "friends-of-behat/mink-debug-extension": "^2.0",
    "friends-of-behat/mink-extension": "^2.4",
    "friends-of-behat/page-object-extension": "^0.3.2",
    "friends-of-behat/symfony-extension": "^2.1",
    "gotenberg/gotenberg-php": "^2.6",
    "phpstan/phpstan": "^1.10",
    "phpstan/phpstan-doctrine": "^1.3.40",
    "phpstan/phpstan-symfony": "^1.3.2",
    "phpstan/phpstan-webmozart-assert": "^1.2.4",
    "phpunit/phpunit": "^10.0",
    "robertfausk/behat-panther-extension": "^1.0",
    "symfony/panther": "^2.1",
    "symfony/runtime": "^6.3",
    "symplify/easy-coding-standard": "^11.1",
    "vimeo/psalm": "^5.23"
  },
  "conflict": {
    "jms/serializer-bundle": "4.1.0",
    "twig/twig": ">=3.9.0 <3.14.0",
    "pimcore/pimcore": "11.3.1"
  },
  "autoload": {
    "psr-4": {
      "CoreShop\\Bundle\\": "src/CoreShop/Bundle/",
      "CoreShop\\Component\\": "src/CoreShop/Component/",
      "CoreShop\\Behat\\": "src/CoreShop/Behat/"
    },
    "exclude-from-classmap": [
      "src/CoreShop/*/*/Tests/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Pimcore\\Model\\DataObject\\": [
        "var/classes/DataObject",
        "var/tmp/behat/var/classes/DataObject"
      ]
    },
    "classmap": [
      "src/Kernel.php",
      "src/BehatKernel.php",
      "src/AppKernel.php"
    ]
  },
  "extra": {
    "symfony-bin-dir": "bin",
    "symfony-var-dir": "var",
    "symfony-tests-dir": "tests",
    "symfony-assets-install": "relative",
    "branch-alias": {
      "dev-next": "4.2-dev",
      "dev-4.1": "4.2-dev"
    }
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "phpstan/extension-installer": false,
      "symfony/runtime": true,
      "php-http/discovery": true
    }
  }
}
