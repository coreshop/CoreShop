{
  "name": "coreshop/resource-bundle",
  "type": "pimcore-bundle",
  "description": "CoreShop - Resource Bundle",
  "keywords": [
    "coreshop",
    "pimcore",
    "ecommerce"
  ],
  "homepage": "http://www.coreshop.org",
  "license": "GPL-3.0-or-later",
  "authors": [
    {
      "name": "Dominik Pfaffenbauer",
      "email": "dominik@pfaffenbauer.at",
      "homepage": "https://www.pfaffenbauer.at/",
      "role": "Developer"
    }
  ],
  "require": {
    "doctrine/orm": "^2.5",
    "doctrine/doctrine-bundle": "^2.2",
    "jms/serializer-bundle": "^3.0",
    "stof/doctrine-extensions-bundle": "^1.2",
    "coreshop/pimcore-bundle": "^3.0",
    "coreshop/optimistic-entity-lock-bundle": "^3.0",
    "coreshop/registry": "^3.0",
    "coreshop/resource": "^3.0",
    "symfony/config": "^5.2.0",
    "symfony/expression-language": "^5.2.0",
    "symfony/form": "^5.2.0",
    "symfony/framework-bundle": "^5.2.0",
    "symfony/security-csrf": "^5.2.0",
    "symfony/templating": "^5.2.0",
    "symfony/twig-bundle": "^5.2.0",
    "symfony/validator": "^5.2.0",
    "symfony/yaml": "^5.2.0",
    "gedmo/doctrine-extensions": "^3.0.0",
    "pimcore/pimcore": "^10.0.0"
  },
  "require-dev": {
    "phpstan/phpstan": "^0.12",
    "phpstan/phpstan-doctrine": "^0.12",
    "phpstan/phpstan-webmozart-assert": "^0.12"
  },
  "autoload": {
    "psr-4": {
      "CoreShop\\Bundle\\ResourceBundle\\": ""
    }
  },
  "repositories": [
      {
          "type": "path",
          "url": "../../*/*"
      }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "3.0-dev"
    }
  }
}
