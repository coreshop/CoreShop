{
  "name": "coreshop/resource-bundle",
  "type": "symfony-bundle",
  "description": "CoreShop - Resource Bundle",
  "keywords": [
    "coreshop",
    "pimcore",
    "ecommerce"
  ],
  "homepage": "http://www.coreshop.org",
  "license": "GPLv3",
  "authors": [
    {
      "name": "Dominik Pfaffenbauer",
      "email": "dominik@pfaffenbauer.at",
      "homepage": "https://www.pfaffenbauer.at/",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.0",
    "doctrine/orm": "^2.5",
    "doctrine/doctrine-bundle": "^1.6",
    "friendsofsymfony/rest-bundle": "^2.1",
    "jms/serializer-bundle": "^1.1",
    "stof/doctrine-extensions-bundle": "^1.2",
    "coreshop/registry": "^2.0",
    "coreshop/resource": "^2.0",
    "symfony/config": "^3.3",
    "symfony/expression-language": "^3.3",
    "symfony/form": "^3.3",
    "symfony/framework-bundle": "^3.3",
    "symfony/security-csrf": "^3.3",
    "symfony/templating": "^3.3",
    "symfony/twig-bundle": "^3.3",
    "symfony/validator": "^3.3",
    "symfony/yaml": "^3.3",
    "gedmo/doctrine-extensions": "^2.4.12"
  },
  "config": {
    "bin-dir": "bin"
  },
  "autoload": {
    "psr-4": {
      "CoreShop\\Bundle\\ResourceBundle\\": ""
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "2.0-dev"
    }
  }
}
