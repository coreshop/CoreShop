{
    "name": "coreshop/resource-bundle",
    "type": "pimcore-bundle",
    "description": "CoreShop - Resource Bundle",
    "keywords": ["coreshop", "pimcore", "ecommerce"],
    "homepage": "http://www.coreshop.org",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Dominik Pfaffenbauer",
            "email": "dominik@pfaffenbauer.at",
            "homepage": "https://www.pfaffenbauer.at/",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2",
        "doctrine/orm": "^2.5",
        "doctrine/doctrine-bundle": "^1.11.0",
        "jms/serializer-bundle": "^2.0",
        "stof/doctrine-extensions-bundle": "^1.2",
        "coreshop/pimcore-bundle": "^3.0",
        "coreshop/registry": "^3.0",
        "coreshop/resource": "^3.0",
        "gedmo/doctrine-extensions": "^2.4.12",
        "pimcore/pimcore": "^7.0.0"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": {
            "CoreShop\\Bundle\\ResourceBundle\\": ""
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "3.0-dev"
        }
    }
}
