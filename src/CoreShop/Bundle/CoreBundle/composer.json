{
  "name": "coreshop/core-bundle",
  "type": "pimcore-bundle",
  "description": "CoreShop - Core Bundle (Glue Bundle)",
  "keywords": [
    "coreshop",
    "pimcore",
    "ecommerce"
  ],
  "homepage": "http://www.coreshop.org",
  "license": "GPL-3.0-or-later",
  "authors": [
    {
      "name": "Dominik Pfaffenbauer",
      "email": "dominik@pfaffenbauer.at",
      "homepage": "https://www.pfaffenbauer.at/",
      "role": "Developer"
    }
  ],
  "require": {
    "coreshop/order-bundle": "^3.0",
    "coreshop/customer-bundle": "^3.0",
    "coreshop/inventory-bundle": "^3.0",
    "coreshop/product-bundle": "^3.0",
    "coreshop/product-quantity-price-rules-bundle": "^3.0",
    "coreshop/address-bundle": "^3.0",
    "coreshop/currency-bundle": "^3.0",
    "coreshop/money-bundle": "^3.0",
    "coreshop/taxation-bundle": "^3.0",
    "coreshop/store-bundle": "^3.0",
    "coreshop/index-bundle": "^3.0",
    "coreshop/payment-bundle": "^3.0",
    "coreshop/seo-bundle": "^3.0",
    "coreshop/shipping-bundle": "^3.0",
    "coreshop/tracking-bundle": "^3.0",
    "coreshop/frontend-bundle": "^3.0",
    "coreshop/sequence-bundle": "^3.0",
    "coreshop/payum-bundle": "^3.0",
    "coreshop/payum-payment-bundle": "^3.0",
    "coreshop/configuration-bundle": "^3.0",
    "coreshop/notification-bundle": "^3.0",
    "coreshop/fixture-bundle": "^3.0",
    "coreshop/workflow-bundle": "^3.0",
    "coreshop/menu-bundle": "^3.0",
    "coreshop/user-bundle": "^3.0",
    "coreshop/core": "^3.0",
    "fzaninotto/faker": "^1.6",
    "rinvex/countries": "^7.0",
    "pimcore/pimcore": "^10.0.0"
  },
  "require-dev": {
    "phpstan/phpstan": "^0.12",
    "phpstan/phpstan-doctrine": "^0.12",
    "phpstan/phpstan-webmozart-assert": "^0.12"
  },
  "autoload": {
    "psr-4": {
      "CoreShop\\Bundle\\CoreBundle\\": ""
    }
  },
  "repositories": [
    {
      "type": "path",
      "url": "../../*/*"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "3.0-dev"
    }
  }
}
