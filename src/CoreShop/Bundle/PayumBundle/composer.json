{
  "name": "coreshop/payum-bundle",
  "type": "pimcore-bundle",
  "description": "CoreShop - Payment Bundle",
  "keywords": [
    "coreshop",
    "pimcore",
    "ecommerce"
  ],
  "homepage": "http://www.coreshop.org",
  "license": "GPL-3.0-or-later",
  "authors": [
    {
      "name": "CoreShop Team",
      "email": "info@coreshop.org",
      "homepage": "https://www.coreshop.org/",
      "role": "Developer"
    },
    {
      "name": "Dominik Pfaffenbauer",
      "email": "dominik@pfaffenbauer.at",
      "homepage": "https://www.pfaffenbauer.at/",
      "role": "Developer"
    }
  ],
  "require": {
    "coreshop/order-bundle": "^4.0",
    "coreshop/payment-bundle": "^4.0",
    "coreshop/payum-payment-bundle": "^4.0",
    "coreshop/core": "^4.0",
    "payum/payum-bundle": "^2.5",
    "payum/payum": "^1.7",
    "php-http/message-factory": "^1.0",
    "symfony/notifier": "^6.3",
    "symfony/serializer": "^6.3",
    "php-http/guzzle7-adapter": "^1.0",
    "pimcore/pimcore": "^11.0"
  },
  "require-dev": {
    "phpstan/phpstan": "^1.10",
    "phpstan/phpstan-doctrine": "^1.3.40",
    "phpstan/phpstan-webmozart-assert": "^1.2.4"
  },
  "autoload": {
    "psr-4": {
      "CoreShop\\Bundle\\PayumBundle\\": ""
    }
  },
  "repositories": [
      {
          "type": "path",
          "url": "../../*/*"
      }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-next": "4.0-dev",
      "dev-master": "4.0-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
